using System;

class Program
{
   static void Main(string[] args)
    {
        Console.WriteLine("Type a number");
        int Number = Convert.ToInt32(Console.ReadLine());
        int sum = 0;
        int a = Number / 1000;
        int b = (Number % 1000) / 100;
        int c = (Number % 100) / 10;
        int d = Number % 10;
        int result = a + b + c + d;
        Console.WriteLine("Result is", +sum);
    }
}
